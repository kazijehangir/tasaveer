{
    "$schema": "../gen/schemas/desktop-schema.json",
    "identifier": "shell-allow-phockup",
    "description": "Allow execution of phockup and immich-go commands (PATH and bundled sidecar)",
    "windows": [
        "main"
    ],
    "permissions": [
        {
            "identifier": "shell:allow-execute",
            "allow": [
                {
                    "name": "phockup",
                    "cmd": "phockup",
                    "args": true
                },
                {
                    "name": "phockup.bat",
                    "cmd": "phockup.bat",
                    "args": true
                },
                {
                    "name": "phockup.exe",
                    "cmd": "phockup.exe",
                    "args": true
                },
                {
                    "name": "immich-go",
                    "cmd": "immich-go",
                    "args": true
                },
                {
                    "name": "immich-go.exe",
                    "cmd": "immich-go.exe",
                    "args": true
                },
                {
                    "name": "binaries/immich-go",
                    "sidecar": true,
                    "args": true
                }
            ]
        },
        {
            "identifier": "shell:allow-spawn",
            "allow": [
                {
                    "name": "phockup",
                    "cmd": "phockup",
                    "args": true
                },
                {
                    "name": "phockup.bat",
                    "cmd": "phockup.bat",
                    "args": true
                },
                {
                    "name": "phockup.exe",
                    "cmd": "phockup.exe",
                    "args": true
                },
                {
                    "name": "immich-go",
                    "cmd": "immich-go",
                    "args": true
                },
                {
                    "name": "immich-go.exe",
                    "cmd": "immich-go.exe",
                    "args": true
                },
                {
                    "name": "binaries/immich-go",
                    "sidecar": true,
                    "args": true
                }
            ]
        },
        "shell:allow-kill"
    ]
}