{
    "$schema": "../gen/schemas/desktop-schema.json",
    "identifier": "shell-allow-phockup",
    "description": "Allow execution of phockup command",
    "windows": [
        "main"
    ],
    "permissions": [
        {
            "identifier": "shell:allow-execute",
            "allow": [
                {
                    "name": "phockup",
                    "cmd": "phockup",
                    "args": true
                }
            ]
        }
    ]
}
